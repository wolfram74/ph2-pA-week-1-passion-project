<div class="spacing">
<h1 class="start_header">Sign in, Lickety Split like.</h1>
<div class="errors">
  <%= @user.errors.full_messages if @user.errors.any? %>

</div>
<form action="/session/new" method="post">
  <input type="text" name="user[name]" placeholder="Username"><br>
  <input type="password" name="user[password]" placeholder="Password">
  <br><br>
  <left><input type="submit" value="Sign in"></left>
</form>
</div>
