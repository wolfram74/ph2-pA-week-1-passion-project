<div class="spacing">
<h1 class="start_header">Register</h1>
<div class="errors">
  <%= @user.errors.full_messages if @user.errors.any? %>

</div>
<form action="/users/new" method="post">
  <input type="text" name="user[name]" placeholder="Username"><br>
  <input type="password" name="user[password]" placeholder="Password">
  <input type="password" name="user[password_confirmation]" placeholder="Confirm">
  <br><br>
  <left><input type="submit" value="Register"></left>
</form>
</div>
